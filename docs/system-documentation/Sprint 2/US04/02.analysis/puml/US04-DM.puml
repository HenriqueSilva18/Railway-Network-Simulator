@startuml
skinparam shadowing false
skinparam classAttributeIconSize 0
skinparam class {
  BackgroundColor LightBlue
  BorderColor Blue
}
skinparam enum {
  BackgroundColor LightBlue
  BorderColor Blue
}
hide circle
hide methods

title Domain Model Excerpt: US04 - Create a Scenario

class Editor {
  username
  password
  createdMaps
  createdScenarios
}

class Scenario {
  nameID
  editor
  startDate
  endDate
  tweakedCityList
  availableIndustryList
  availableLocomotives
}

class Map {
  nameID
  scenarios
  cityList
  industryList
}

class Industry {
  nameID
  type
  availabilityYear
  productionRate
  suppliedCargo
  demandedCargo
}

class IndustrySector {
  name
  importsExports
  generatesCargo
  consumesCargo
  description
}

class Cargo {
  name
  amount
  lifespan
  type
  productionResources
}

class Locomotive {
  nameID
  owner
  type
  power
  acceleration
  topSpeed
  startYear
  fuelCost
  availabilityYear
  acquisitionPrice
  maintenancePrice
}

class City {
  nameID
  trafficRate
  suppliedCargo
  demandedCargo
}

Editor "1" -- "*" Map : creates >
Editor "1" -- "*" Scenario : creates >
Scenario "*" -- "1" Map : uses >
Map "1" -- "*" Industry : contains >
Industry "*" -- "*" Cargo : produces/consumes >
Scenario "1" -- "*" Locomotive : configures >
Scenario "1" -- "*" Industry : configures >
City "*" -- "*" Cargo : produces/consumes >
Map "1" -- "*" City : contains >
Scenario "1" -- "*" City : contains >
IndustrySector "1" -- "*" Industry : contains >

@enduml