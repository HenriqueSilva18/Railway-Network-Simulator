@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

participant ":CreateScenarioController" as CTRL
participant "map\n:Map" as MAP

autonumber 9
-> CTRL : getMapIndustries(selectedMap)

autonumber 9.1
activate CTRL
    group Get Map Industries List
        CTRL -> MAP : getIndustries()
        activate MAP
            MAP --> CTRL : industryList
        deactivate MAP
    end

    autonumber 10
    <-- CTRL : industryList
deactivate CTRL

@enduml 